class Solution {
    public int[] createTargetArray(int[] nums, int[] index) {
        ArrayList<Integer> tmp = new ArrayList<>();
        int res[] = new int[nums.length];
        for(int i =0;i<nums.length;i++){
            tmp.add(index[i], nums[i]);
        }
        for(int j=0; j<nums.length;j++){
            res[j]= tmp.get(j);
        }
        return res;
    }
}
